{
    const t = CGL.ShaderGraphOp.getMaxGenTypeFromPorts([inputb, inputa]);

    inputa.setUiAttribs({ "objType": t });
    outvec.setUiAttribs({ "objType": t });
    inputb.setUiAttribs({ "objType": t });
};

op.shaderCodeOperator = "*";


