{
    mod.toggleDefine("MOD_FLIPX",inFlipX.get());
    mod.toggleDefine("MOD_FLIPY",inFlipY.get());
}

render.onTriggered = function ()
{
    mod.bind();
    next.trigger();
    mod.unbind();
};


};

