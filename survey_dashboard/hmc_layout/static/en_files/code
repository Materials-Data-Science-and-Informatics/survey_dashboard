Ops.Json.ObjectValues = function()
{
CABLES.Op.apply(this,arguments);
const op=this;
const attachments={};
const
    inObj = op.inObject("Object"),
    outNumValues = op.outNumber("Num values"),
    outValues = op.outArray("Values");

